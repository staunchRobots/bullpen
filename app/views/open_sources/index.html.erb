<h1>Listing open_sources</h1>

<table>
  <thead>
    <tr>
      <th>Cv</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @open_sources.each do |open_source| %>
      <tr>
        <td><%= open_source.cv %></td>
        <td><%= link_to 'Show', open_source %></td>
        <td><%= link_to 'Edit', edit_open_source_path(open_source) %></td>
        <td><%= link_to 'Destroy', open_source, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Open source', new_open_source_path %>
